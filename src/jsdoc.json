{
  "source": {
    "include": ["components", "components/answer","components/auth/forgotpassword"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",         
    "recurse": true
  }
}
